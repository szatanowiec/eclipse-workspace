package pl.javastart.witaj;
import java.util.Scanner;

public class Witaj {

	public static void main(String[] args) {
		String powitanie = "Podaj swoje imie";
		String imie; //tutaj zapiszemy imiê
		Scanner odczyt = new Scanner(System.in); //obiekt do odebrania danych od u¿ytkownika
		
		imie = odczyt.nextLine();
		System.out.println(powitanie);
		switch(imie) {
		case"Micha³":
			System.out.println("Witaj "+imie);
			break;
		case"£ukasz":
			System.out.println("Witaj "+imie);
			break;
		case"Marcin":
			System.out.println("Witaj "+imie);
			break;
		default:
			System.out.println("Chyba siê nie znamy :)");
		}		
		
	}

}
